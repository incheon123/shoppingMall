<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
	<display-name>Triple_kim3</display-name>
	<welcome-file-list>
		<welcome-file>/WEB-INF/view/index.jsp</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>ParsePathController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/view/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>membership</servlet-name>
		<servlet-class>ParsePathController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>membership</servlet-name>
		<url-pattern>/view/membership</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>basket</servlet-name>
		<servlet-class>ParsePathController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>basket</servlet-name>
		<url-pattern>/view/basket</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>mypage</servlet-name>
		<servlet-class>ParsePathController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>mypage</servlet-name>
		<url-pattern>/view/mypage/profile</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>product</servlet-name>
		<servlet-class>ProductController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>product</servlet-name>
		<url-pattern>/product/*</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>loginDo</servlet-name>
		<servlet-class>LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>loginDo</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>logoutDo</servlet-name>
		<servlet-class>LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>logoutDo</servlet-name>
		<url-pattern>/logout.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>createAccount</servlet-name>
		<servlet-class>RegisterController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>createAccount</servlet-name>
		<url-pattern>/shit/create/account</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>checkDuplicate</servlet-name>
		<servlet-class>RegisterController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>checkDuplicate</servlet-name>
		<url-pattern>/view/check/duplicate</url-pattern>
	</servlet-mapping>
</web-app>